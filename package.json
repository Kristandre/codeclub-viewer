{
  "name": "codeclub-viewer",
  "version": "1.0.0",
  "description": "A viewer to display codeclub lessons",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --progress",
    "build": "webpack -d --progress",
    "buildprod": "webpack -p",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js \"test/**/*@(.js|.jsx)\"",
    "test:watch": "npm run test -- --watch",
    "eslint": "eslint src/ test/ --ext .js,.jsx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NorwegianKiwi/codeclub-viewer.git"
  },
  "keywords": [
    "codeclub",
    "lesson",
    "viewer"
  ],
  "author": "Lance Olav Eastgate",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NorwegianKiwi/codeclub-viewer/issues"
  },
  "homepage": "https://github.com/NorwegianKiwi/codeclub-viewer#readme",
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ]
  },
  "dependencies": {
    "react": "15.0.2",
    "react-dom": "15.0.2"
  },
  "devDependencies": {
    "babel-core": "6.8.0",
    "babel-eslint": "6.0.4",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-2": "6.5.0",
    "chai": "3.5.0",
    "clean-webpack-plugin": "0.1.9",
    "combine-loader": "0.1.0",
    "eslint": "2.9.0",
    "eslint-plugin-react": "5.1.1",
    "file-loader": "0.8.5",
    "front-matter-loader": "0.1.2",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.16.1",
    "jsdom": "9.0.0",
    "json-loader": "0.5.4",
    "markdown-it-anchor": "2.5.0",
    "markdown-it-attrs": "0.2.1",
    "markdown-it-header-sections": "0.2.2",
    "markdown-it-implicit-figures": "0.2.3",
    "markdown-it-loader": "0.2.0",
    "mocha": "2.4.5",
    "raw-loader": "0.5.1",
    "react-hot-loader": "1.3.0",
    "url-loader": "0.5.7",
    "webpack": "1.13.0",
    "webpack-dev-server": "1.14.0"
  }
}
